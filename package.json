{
  "name": "pixel-dominion",
  "version": "1.0.0",
  "private": true,
  "description": "Real-time pixel-based RTS game with map-based territory control",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "start": "tsx server.ts",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "build:ai:grok": "npm run build && echo 'Grok AI build complete'",
    "build:ai:gemini": "npm run build && echo 'Gemini AI build complete'",
    "build:ai:codex": "npm run build && echo 'Codex AI build complete'",
    "build:ai:qwen": "npm run build && echo 'Qwen AI build complete'",
    "seed-demo": "tsx scripts/seed-demo.ts",
    "perf-check": "tsx scripts/perf-check.ts"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^5.2.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.35.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.0",
    "prettier-plugin-svelte": "^3.2.0",
    "svelte": "^4.2.0",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vite": "^5.1.0"
  },
  "dependencies": {
    "maplibre-gl": "^4.1.0",
    "ws": "^8.16.0",
    "zod": "^3.22.4",
    "@types/ws": "^8.5.10"
  },
  "type": "module"
}